<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="Music_JukeBox">
		<control type="group">
			<visible>Control.IsVisible(603) + !Window.IsActive(addoninformation)</visible>
			<include>ViewAnimation</include>
			<include>WindowOpenCloseAnimation</include>
			<control type="group">
				<top>270</top>
				<control type="group">
					<left>50</left>
					<control type="image">
						<left>-32</left>
						<top>148</top>
						<width>874</width>
						<height>606</height>
						<texture colordiffuse="E6FFFFFF" border="40">listpanel_back.png</texture>
					</control>
					<control type="panel" id="603">
						<top>180</top>
						<width>810</width>
						<height>540</height>
						<onleft>60360</onleft>
						<onright>60350</onright>
						<onup>603</onup>
						<ondown>603</ondown>
						<viewtype label="JukeBox">list</viewtype>
						<pagecontrol>60360</pagecontrol>
						<scrolltime tween="quadratic" easing="out">200</scrolltime>
						<itemlayout height="90" width="810">
							<control type="label">
								<left>15</left>
								<top>5</top>
								<width>775</width>
								<height>70</height>
								<font>font30</font>
								<textcolor>grey2</textcolor>
								<label>$INFO[ListItem.Label]</label>
							</control>
						</itemlayout>
						<focusedlayout height="90" width="690">
							<control type="image">
								<width>810</width>
								<height>90</height>
								<texture colordiffuse="$VAR[HighlightBarColor]" border="4">listselect_fo.png</texture>
								<visible>Control.HasFocus(603)</visible>
								<include>VisibleFadeAnimation</include>
							</control>
							<control type="label">
								<left>15</left>
								<top>5</top>
								<width>775</width>
								<height>70</height>
								<font>font30</font>
								<label>$INFO[ListItem.Label]</label>
							</control>
						</focusedlayout>
					</control>
					<control type="scrollbar" id="60360">
						<left>-18</left>
						<top>180</top>
						<width>14</width>
						<height>540</height>
						<onup>60360</onup>
						<ondown>60360</ondown>
						<onleft>SetProperty(MediaMenu,True,Home)</onleft>
						<onleft>SetFocus(9050)</onleft>
						<onright>603</onright>
						<texturesliderbackground />
						<texturesliderbar colordiffuse="80FFFFFF" border="1">scrollbar/texturesliderbar2.png</texturesliderbar>
						<texturesliderbarfocus border="1">scrollbar/texturesliderbar2.png</texturesliderbarfocus>
						<showonepage>false</showonepage>
					</control>
				</control>
				<control type="group">
					<left>1064</left>
					<control type="image">
						<left>-32</left>
						<top>148</top>
						<width>874</width>
						<height>606</height>
						<texture colordiffuse="E6FFFFFF" border="40">listpanel_back.png</texture>
					</control>
					<control type="panel" id="60350">
						<top>180</top>
						<width>810</width>
						<height>540</height>
						<onleft>603</onleft>
						<onright>61</onright>
						<onup>60350</onup>
						<ondown>60350</ondown>
						<animation effect="fade" start="100" end="0" time="300" condition="Container(60350).IsUpdating">Conditional</animation>
						<viewtype label="535">list</viewtype>
						<pagecontrol>61</pagecontrol>
						<scrolltime tween="quadratic" easing="out">200</scrolltime>
						<itemlayout height="90" width="810">
							<control type="label">
								<left>15</left>
								<top>-10</top>
								<width>655</width>
								<height>70</height>
								<font>font30</font>
								<textcolor>grey</textcolor>
								<label>$INFO[ListItem.TrackNumber,,. ]$INFO[ListItem.Label]</label>
								<animation effect="slide" start="0,0" end="0,25" time="0" condition="String.IsEmpty(ListItem.Album)">Conditional</animation>
							</control>
							<control type="label">
								<left>15</left>
								<top>40</top>
								<width>620</width>
								<height>60</height>
								<font>font15</font>
								<textcolor>grey2</textcolor>
								<label>$INFO[ListItem.Album]</label>
							</control>
							<control type="label">
								<left>60</left>
								<top>-10</top>
								<width>730</width>
								<height>60</height>
								<font>font15</font>
								<align>right</align>
								<textcolor>grey2</textcolor>
								<label>$INFO[ListItem.Duration]</label>
							</control>
							<control type="image">
								<left>645</left>
								<top>40</top>
								<width>150</width>
								<height>60</height>
								<aspectratio>keep</aspectratio>
								<colordiffuse>$VAR[ThemeLabelColor]</colordiffuse>
								<texture fallback="rating0.png">$INFO[ListItem.UserRating,flags/rating/,.png]</texture>
							</control>
						</itemlayout>
						<focusedlayout height="90" width="810">
							<control type="image">
								<width>810</width>
								<height>90</height>
								<texture colordiffuse="$VAR[HighlightBarColor]" border="4">listselect_fo.png</texture>
								<visible>Control.HasFocus(60350)</visible>
								<include>VisibleFadeAnimation</include>
							</control>
							<control type="label">
								<left>15</left>
								<top>-10</top>
								<width>655</width>
								<height>70</height>
								<font>font30</font>
								<label>$INFO[ListItem.TrackNumber,,. ]$INFO[ListItem.Label]</label>
								<animation effect="slide" start="0,0" end="0,10" time="0" condition="String.IsEmpty(ListItem.Album)">Conditional</animation>
							</control>
							<control type="label">
								<left>15</left>
								<top>40</top>
								<width>500</width>
								<height>60</height>
								<font>font15</font>
								<textcolor>grey2</textcolor>
								<label>$INFO[ListItem.Album]</label>
							</control>
							<control type="label">
								<left>60</left>
								<top>-10</top>
								<width>730</width>
								<height>60</height>
								<font>font15</font>
								<align>right</align>
								<textcolor>grey2</textcolor>
								<label>$INFO[ListItem.Duration]</label>
							</control>
							<control type="image">
								<left>645</left>
								<top>40</top>
								<width>150</width>
								<height>60</height>
								<aspectratio>keep</aspectratio>
								<colordiffuse>$VAR[ThemeLabelColor]</colordiffuse>
								<texture fallback="rating0.png">$INFO[ListItem.UserRating,flags/rating/,.png]</texture>
							</control>
						</focusedlayout>
						<content target="music">$VAR[JukeBoxContentPathVar]</content>
					</control>
					<control type="scrollbar" id="61">
						<left>810</left>
						<top>180</top>
						<width>14</width>
						<height>540</height>
						<onup>61</onup>
						<ondown>61</ondown>
						<onleft>60350</onleft>
						<onright>SetProperty(MediaMenu,True,Home)</onright>
						<onright>SetFocus(9050)</onright>
						<texturesliderbackground />
						<texturesliderbar colordiffuse="80FFFFFF" border="1">scrollbar/texturesliderbar2.png</texturesliderbar>
						<texturesliderbarfocus border="1">scrollbar/texturesliderbar2.png</texturesliderbarfocus>
						<showonepage>false</showonepage>
					</control>
				</control>
			</control>
			<control type="group">
				<top>40</top>
				<left>253</left>
				<control type="image">
					<width>400</width>
					<height>400</height>
					<aspectratio aligny="bottom">keep</aspectratio>
					<texture background="true">$VAR[JukeBoxArtistThumbVar]</texture>
					<fadetime>IconCrossfadeTime</fadetime>
					<bordertexture border="20">thumbs/thumb_shadow.png</bordertexture>
					<bordersize>20</bordersize>
				</control>
				<control type="image">
					<left>1014</left>
					<width>400</width>
					<height>400</height>
					<aspectratio aligny="bottom">keep</aspectratio>
					<texture background="true">$INFO[Container(60350).ListItem.Icon]</texture>
					<fadetime>IconCrossfadeTime</fadetime>
					<bordertexture border="20">thumbs/thumb_shadow.png</bordertexture>
					<bordersize>20</bordersize>
				</control>
			</control>
		</control>
	</include>
</includes>